<resources>
    <declare-styleable name="MultiStateView">
        <attr name="msv_loadingView" format="reference"></attr>
        <attr name="msv_emptyView" format="reference"></attr>
        <attr name="msv_errorView" format="reference"></attr>
        <attr name="msv_viewState" format="integer"></attr>
        <attr name="msv_animateViewChanges" format="boolean"></attr>
    </declare-styleable>


    <declare-styleable name="BadgeImage">
        <attr name="textColor" format="color"></attr>
        <attr name="circleColor" format="color"></attr>
        <attr name="radius" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="AddWidget">
        <attr name="circle_anim" format="boolean" />
        <attr name="sub_anim" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SearchView">
        <attr name="hint" format="string"></attr>
    </declare-styleable>
    <declare-styleable name="SearchEditText">
        <attr name="drawableDel" format="reference" />
    </declare-styleable>
</resources>
